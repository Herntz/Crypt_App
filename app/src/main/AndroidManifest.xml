<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crypt_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crypt_App">
        <activity
            android:name=".Activity.DecryptionActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.TeamActivity"
            android:exported="true"
            android:parentActivityName=".Activity.CryptActivity" />
        <activity
            android:name=".Activity.CryptActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.HomeActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>